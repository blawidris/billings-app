{
  "expo": {
    "name": "paybill",
    "slug": "paybill",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/paybills.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yawdah.paybill"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yawdah.paybill"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.858895247705-10oloqbd5rucogh1omgo1mqbkg00g29k"
        }
      ],[
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "852a94b8-0a33-43a7-a36a-42c33794489a"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/852a94b8-0a33-43a7-a36a-42c33794489a"
    }
  }
}
